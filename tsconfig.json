{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "noImplicitAny": true,
    "outDir": "./dist",
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "ES5",
  }
}
